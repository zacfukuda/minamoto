<footer id="footer" class="footer" role="contentinfo">
	<a href="{{ site.url }}" role="banner" rel="home" title="{{ site.name }}">
		<img src="{{ paths.rel_template }}/img/logo/normal.svg" alt="{{ site.name }}" width="128" height="128">
	</a>
	<nav>
		<ul>
			{% for item in footer_nav.get_items %}
				<li>
					<a href="{{ item.link }}" target="{{ item.target }}">{{ item.title }}</a>
					{% if item.get_children %}
						<ul>
							{% for child in item.get_children %}
								<li>
									<a href="{{child.link}}" target="{{ child.target }}">{{ child.title }}</a>
								</li>
							{% endfor %}
						</ul>
					{% endif %}
				</li>
			{% endfor %}
		</ul>
	</nav>
	<p class="copyright">Â© {{function('date', 'Y')}} Your Company, Inc. All rights reserved.</p>
</footer>
