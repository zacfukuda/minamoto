<header id="gheader" class="gheader">
	<a href="{{ site.url }}" role="banner" rel="home" title="{{ site.name }}">
		<img src="{{ paths.rel_template }}/img/logo/normal.svg" alt="{{ site.name }}" width="128" height="128">
	</a>
	{% include 'component/gnav.twig' %}
	<button class="toggle">
		<svg class="hamburger" width="24" height="16" viewBox="0 0 24 16">
			<rect x="0" y="0" width="24" height="2" />
			<rect x="0" y="7" width="24" height="2" />
			<rect x="0" y="14" width="24" height="2" />
		</svg>
	</button>
</header>
