<header id="header" class="header">
	<a href="{{ site.url }}" role="banner" rel="home" title="{{ site.name }}">
		<img src="{{ paths.rel_template }}/img/logo/normal.svg" alt="{{ site.name }}" width="128" height="128">
	</a>
	<nav role="navigation">
		{% include "component/menu.twig" with {
			'items': global_nav.get_items,
			'class_name': ''
		} %}
	</nav>
	<button class="toggle">Toggle</button>
</header>
